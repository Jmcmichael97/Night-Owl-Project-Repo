@model DWC_NightOwlProject.Data.Material


<html>


<head>
         
    
   
    </head>
<body>
<div class="text-center" id="title">
    <h2>Map Creation Result</h2>
</div>

<div class="text-center" id="adviceBox">
    <span>Map is power!</span>
</div>







<h4>
    Completion:  <br />
</h4>

<a>

        <iframe style="width: 1000px; height: 1000px; overflow: hidden;" scrolling="no" frameborder="0" src=@Model.Completion></iframe>
</a>

<canvas id="myCanvas" width="256" height="256">

          

</canvas>


@*@Model.Completion*@

<br />

<script src = "https://cdnjs.cloudflare.com/ajax/libs/FileSaver.js/1.3.8/FileSaver.min.js"></script>

<a href="@Url.Action("Save","Maps", Model)">Save</a>  <a href="@Url.Action("Index","Maps")">Discard</a>

<button class="btn">Download</button>

<script>
    const btnEl = document.querySelector('.btn');

    btnEl.addEventListener('click', saveImage);

    function saveImage(){

        const content = "@Html.Raw(Model.Completion).ToString().Substring(2)";
        var blob = new Blob([content], { type: "image/png" });
        @Model.PictureData = blob;
    }

       
            
            
    </script>

   

    </body>

</html>







